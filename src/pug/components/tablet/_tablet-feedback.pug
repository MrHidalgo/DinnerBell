						span(changeColor-notGood-js)!= "NOT GOOD"
				.tablet__rating-bottom
					a.is-choose(href="#" class="" title="")
						i.icon-font.icon-rate
					a.is-choose(href="#" class="" title="")
						i.icon-font.icon-rate
					a(href="#" class="" title="")
						i.icon-font.icon-rate
					a(href="#" class="" title="")
						i.icon-font.icon-rate
					a(href="#" class="" title="")
						i.icon-font.icon-rate

			.tablet__rating.tablet__rating--bad
				.tablet__rating-top
					div
						p!= "Rate your overall experience"
					div
						span(changeColor-bad-js)!= "bad"
				.tablet__rating-bottom
