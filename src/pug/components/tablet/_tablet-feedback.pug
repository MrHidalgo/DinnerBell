						i.icon-font.icon-rate

			.tablet__rating.tablet__rating--verygood
				.tablet__rating-top
					div
						i!= "Cold appetizers"
						p!= "Chilled Shellfish Platter"
					div
						span(changeColor-veryGood-js)!= "VERY GOOD"
				.tablet__rating-bottom
					a.is-choose(href="#" class="" title="")
						i.icon-font.icon-rate
					a.is-choose(href="#" class="" title="")
