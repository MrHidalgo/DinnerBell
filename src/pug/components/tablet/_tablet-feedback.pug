section#tablet.tablet.tablet--feedback

	.tablet__header
		div
			a(href="#" class="" title="")
				span
				span
				span
		div
			a(href="" class="" title="")
				i.icon-font.icon-user
				span!= "3"
		div
			a(href="" class="" title="")
				i.icon-font.icon-doc
				span!= "0"
		div
			a(href="" class="" title="")
				i.icon-font.icon-bell

	.tablet__content
		.tablet__banner

		.tablet__rating-wrapper
			.tablet__rating.tablet__rating--excellent
				.tablet__rating-top
					div
						i!= "WINE"
						p!= "Cakebread Cellars, Merlot"
					div
						span(changeColor-excellent-js)!= "Excellent"
				.tablet__rating-bottom
					a.is-choose(href="#" class="" title="")
