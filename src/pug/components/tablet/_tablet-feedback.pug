						span(changeColor-good-js)!= "GOOD"
				.tablet__rating-bottom
					a.is-choose(href="#" class="" title="")
						i.icon-font.icon-rate
					a.is-choose(href="#" class="" title="")
						i.icon-font.icon-rate
					a.is-choose(href="#" class="" title="")
						i.icon-font.icon-rate
					a(href="#" class="" title="")
						i.icon-font.icon-rate
					a(href="#" class="" title="")
						i.icon-font.icon-rate

			.tablet__rating.tablet__rating--notgood
				.tablet__rating-top
					div
						p!= "Please rate our service efficiency"
