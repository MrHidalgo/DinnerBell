// SECTION tablet :: START

section#tablet.tablet.tablet--mainMenu
	.tablet__header
		div
			a(href="#" class="" title="")
				span
				span
				span
		div
			a(href="" class="" title="")
				i.icon-font.icon-user
				span!= "3"
		div
			a(href="" class="" title="")
				i.icon-font.icon-doc
				span!= "0"
		div
			a(href="" class="" title="")
				i.icon-font.icon-bell

	.tablet__content

		.tablet__carousel
			mixin swiperContent()
				.swiper-slide.swiper-slide-0
					p!= "Breakfast"
					span!= "Serving 7 am to 11 am"
				.swiper-slide.swiper-slide-1
					p!= "Breakfast"
					span!= "Serving 7 am to 11 am"
				.swiper-slide.swiper-slide-2
					p!= "Breakfast"
					span!= "Serving 7 am to 11 am"

			+swiper('swiper-container-tabletcarousel', 'swiperContent', false, true, false)

		.tablet__content-cover(tablet-bg-js)

			.tablet__block-wrapper.tablet__block-wrapper--thumbnail.is-thumbnail
				a.tablet__block(href="#" class="" title="")
					div
						img(src="img/img-tablet-box.jpg" alt="" title="")
					div
						p(thumbnail-js)!= "Appetizers"
				a.tablet__block(href="#" class="" title="")
					div
						img(src="img/img-tablet-box.jpg" alt="" title="")
					div
						p(thumbnail-js)!= "Soups & Salads"
				a.tablet__block(href="#" class="" title="")
					div
						img(src="img/img-tablet-box.jpg" alt="" title="")
					div
						p(thumbnail-js)!= "Classic Favorites"
				a.tablet__block(href="#" class="" title="")
					div
						img(src="img/img-tablet-box.jpg" alt="" title="")
					div
						p(thumbnail-js)!= "Taste of Mediterranian"
				a.tablet__block(href="#" class="" title="")
					div
						img(src="img/img-tablet-box.jpg" alt="" title="")
					div
						p(thumbnail-js)!= "Chicken"
				a.tablet__block(href="#" class="" title="")
					div
						img(src="img/img-tablet-box.jpg" alt="" title="")
					div
						p(thumbnail-js)!= "Seafood"

			.tablet__block-wrapper.tablet__block-wrapper--list.is-list
				a.tablet__block(href="#" class="" title="")
					div
						p(list-js)!= "Appetizers"
				a.tablet__block(href="#" class="" title="")
					div
						p(list-js)!= "Soups & Salads"
				a.tablet__block(href="#" class="" title="")
					div
						p(list-js)!= "Classic Favorites"
				a.tablet__block(href="#" class="" title="")
					div
						p(list-js)!= "Taste of Mediterranian"
				a.tablet__block(href="#" class="" title="")
					div
						p(list-js)!= "Chicken"
				a.tablet__block(href="#" class="" title="")
					div
						p(list-js)!= "Seafood"

			.tablet__info-wrapper
				p.tablet__info-title!= 'Information'

				a.tablet__info-link(href="#" class="" title="" information-js)!= "History & The Team"
				a.tablet__info-link(href="#" class="" title="" information-js)!= "Loyalty program"
				a.tablet__info-link(href="#" class="" title="" information-js)!= "Upcoming events"

				img.tablet__info-logo(src="img/img-tablet-sublogo.png" srcset="img/img-tablet-sublogo@2x.png 2x" alt="" title="")

				p.tablet__info!= "powered by dinnerbell"



// SECTION tablet :: END
